<%- include('../views/layout/layout') %>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="error-template">
                <h1><%= message %></h1>
                <h2><%= error.status %></h2>
                <div class="error-details">
                    Sorry, an error has occured, Requested page not found!
                    <pre><%= error.stack %></pre>
                </div>
                <div class="error-actions">
                    <a href="/" class="btn btn-primary btn-lg">Take Me Home </a>
                </div>
            </div>
        </div>
    </div>
</div>